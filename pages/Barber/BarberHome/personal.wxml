<!-- 个人信息 -->
<view wx:if="{{person[curIndex]}}">
        <block wx:for="{{person[curIndex]}}" wx:key="item.id">
<view class="summary">
	<!-- <view class="avatar-container"> -->
		<image src="{{item.cover}}" class="avatar" />
	<!-- </view> -->
  <view class ="nick">
		<view class="nickname">{{item.name}}</view>
    <view class="nickname1" >{{item.level}}|<text class="nickname1">{{item.exp}}</text></view>
    </view>
</view>
</block>
</view>

<!-- 账户 -->
<view wx:if="{{person[curIndex]}}">
        <block wx:for="{{person[curIndex]}}" wx:key="item.id">
<view class="account">
	<view class="item">
		<view class="value balance">{{item.income}}</view>
		<view class="caption">我的收入</view>
	</view>
	<view class="item">
		<view class="value coupon">{{item.order}}</view>
		<view class="caption">我的订单</view>
	</view>
	<view class="item">
		<view class="value credit">{{item.custumerNum}}</view>
		<view class="caption">我的顾客</view>
	</view>
</view>
</block>
</view>
