.myBarberList{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  background-color: #ececec
}
.list{
   display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  width: 100%;
  height:220rpx;
    border-bottom:1px solid #ececec;
    background-color: white;
}
.img{
  width: 120rpx;
  height: 120rpx;
  border:1px solid #ececec;
  border-radius: 70rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 20rpx;

}
.barberName{
  font-size: 30rpx;
  margin-bottom: 8rpx;
  color:#333333;
}
.text{
  font-size: 26rpx;
   margin-bottom: 5rpx;
   color:#999999;
}
.content{
  display:flex;
  flex-direction: column;
  margin-top: auto;
  margin-bottom: auto;
}
